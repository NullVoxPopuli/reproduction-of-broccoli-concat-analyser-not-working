{
  "name": "modern-ember",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for modern-ember goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "clean": "rm -rf dist tmp node_modules concat-stats-for coverage*",
    "ember": "EMBER_CLI_BROCCOLI_2=true EMBER_CLI_SYSTEM_TEMP=true EMBER_CLI_MODULE_UNIFICATION=true node_modules/.bin/ember",
    "ember:update": "ember-cli-update",
    "build": "yarn ember build",
    "build:debug": "EMBER_CLI_PACKAGER=true BROCCOLI_DEBUG=* yarn build",
    "build:production": "ENABLE_SW=true PREMBER=true EXPERIMENTAL_RENDER_MODE_SERIALIZE=true yarn build --environment production && sed -i -e 's/{{ROOT_URL}}/\\//g' dist/index.html",
    "build:production:analyze": "CONCAT_STATS=true yarn build:production",
    "start": "yarn ember serve -p 4201",
    "start:dev": "ENABLE_SW=false SW_DISABLED=true FASTBOOT_DISABLED=true yarn start -p 4201",
    "test": "yarn ember exam --random",
    "test:named": "COVERAGE=true yarn ember exam --partition 1 --split 1 --filter ",
    "test:server": "yarn ember exam --server",
    "merge-coverage": "ember coverage-merge",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "yarn eslint './**/*.ts'",
    "lint:sass": "yarn sass-lint --config .sass-lint.yml src/ui/styles/**/*.scss -q -v",
    "lint:pretty": "yarn prettier \"{config,lib,server,tests/**/*.ts,src/**/*.ts}\"",
    "lint:fix": "bash -c 'yarn lint --fix'",
    "deploy": "yarn ember deploy production",
    "bundle-analyze": "yarn broccoli-concat-analyser ./concat-stats-for",
    "analyze": "yarn build:production:analyze && yarn bundle-analyze && cp ./concat-stats-for/index.html ./public/bundle.html"
  },
  "devDependencies": {
    "@ember/jquery": "^0.5.2",
    "@ember/optional-features": "^0.6.3",
    "broccoli-asset-rev": "^2.7.0",
    "broccoli-concat-analyser": "^4.3.0",
    "ember-ajax": "^3.1.0",
    "ember-cli": "github:ember-cli/ember-cli",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^6.16.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-data": "~3.4.0-beta.1",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^5.0.1",
    "ember-source": "https://s3.amazonaws.com/builds.emberjs.com/canary/shas/49e58e45604bd63ac1001cb491195c032c050053.tgz",
    "ember-welcome-page": "^3.2.0",
    "eslint-plugin-ember": "^5.2.0",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.7.1"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  }
}
